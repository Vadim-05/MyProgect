value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","format","parseISO","ru","ordersData","productsData","orders","products","selectedOrder","productInfo","value","reduce","acc","product","id","title","price","map","p","symbol","join","getProductCount","orderId","order","find","productIds","length","formatDate","date","formatStr","locale","formatShortDate","formatLongDate","selectOrder"],"sources":["/home/vadim/my-test/src/views/HomePrihod.vue"],"sourcesContent":["<template>\n  <div>\n    <ul>\n      <li class=\"order-item\" v-for=\"order in orders\" :key=\"order.id\" @click=\"selectOrder(order)\">\n        <span class=\"order-title\">Длиное название прихода</span>\n        <div class=\"order-info\">\n          <div>Кількість продуктів: {{ getProductCount(order.id) }}</div>\n          <div>Продукти</div>\n        </div>\n        <div class=\"order-info\">\n          <div>{{ formatShortDate(order.date) }}</div>\n          <div>{{ formatLongDate(order.date) }}</div>\n        </div>\n\n        <div class=\"vfdsvfdcs\">\n          <ul>\n            <li v-for=\"productId in order.productIds\" :key=\"productId\">\n              <div>{{ productInfo[productId].title || 'No product' }}</div>\n              <div class=\"product-price\">{{ productInfo[productId].price || 'No price' }}</div>\n              <!-- Додатково відображаємо інформацію про продукт при виборі -->\n              <div v-if=\"selectedOrder && selectedOrder.id === order.id\">\n                <p>Деталі продукту:</p>\n                <ul>\n                  <li v-for=\"productId in order.productIds\" :key=\"productId\">\n                  \t<div>Ноутбук игровой Acer Nitro 5 AN515-58 (NH.QFHEU.004) Obsidian Black</div>\n                    <div>{{ productInfo[productId].title || 'No product' }}</div>\n                    <div class=\"product-price\">{{ productInfo[productId].price || 'No price' }}</div>\n                  </li>\n                </ul>\n              </div>\n            </li>\n          </ul>\n        </div>\n      </li>\n    </ul>\n  </div>\n</template>\n\n<script setup>\nimport { ref, computed } from 'vue';\nimport { format, parseISO } from 'date-fns';\nimport { ru } from 'date-fns/locale';\nimport ordersData from '../data/orders.json';\nimport productsData from '../data/products.json';\n\n// Створюємо реактивні змінні\nconst orders = ref(ordersData);\nconst products = ref(productsData);\nconst selectedOrder = ref(null);\n\n// Функція для отримання даних про продукт\nconst productInfo = computed(() => {\n  return products.value.reduce((acc, product) => {\n    acc[product.id] = {\n      title: product.title || 'No product',\n      price: product.price?.map(p => `${p.value} ${p.symbol}`).join(', ') || 'No price'\n    };\n    return acc;\n  }, {});\n});\n\nconst getProductCount = (orderId) => {\n  const order = orders.value.find(order => order.id === orderId);\n  return order ? order.productIds.length : 0;\n};\n\nconst formatDate = (date, formatStr, locale) => format(parseISO(date), formatStr, { locale });\n\nconst formatShortDate = (date) => formatDate(date, 'MM/dd');\nconst formatLongDate = (date) => formatDate(date, 'dd/MMM/yyyy', ru);\n\nconst selectOrder = (order) => selectedOrder.value = order;\n</script>\n\n\n\n<style scoped>\n.order-item {\n  display: flex;\n  padding: 10px;\n  border: 1px solid #ddd;\n  margin-bottom: 10px;\n  cursor: pointer;\n}\n\n.order-info {\n  padding-left: 30px;\n  display: flex;\n  flex-direction: column;\n}\n\n.order-title {\n  font-weight: bold;\n}\n\n.product-price {\n  color: green;\n  font-weight: bold;\n}\n\n.vfdsvfdcs {\n  padding-left: 40px;\n}\n</style>\n"],"mappings":"AAuCA,SAASA,GAAG,EAAEC,QAAQ,QAAQ,KAAK;AACnC,SAASC,MAAM,EAAEC,QAAQ,QAAQ,UAAU;AAC3C,SAASC,EAAE,QAAQ,iBAAiB;AACpC,OAAOC,UAAU,MAAM,qBAAqB;AAC5C,OAAOC,YAAY,MAAM,uBAAuB;;AAEhD;;;;;;;;IACA,MAAMC,MAAM,GAAGP,GAAG,CAACK,UAAU,CAAC;IAC9B,MAAMG,QAAQ,GAAGR,GAAG,CAACM,YAAY,CAAC;IAClC,MAAMG,aAAa,GAAGT,GAAG,CAAC,IAAI