{"ast":null,"code":"import { ref, computed } from 'vue';\nimport { format, parseISO } from 'date-fns';\nimport { ru } from 'date-fns/locale';\nimport ordersData from '../data/orders.json';\nimport productsData from '../data/products.json';\n\n// Створюємо реактивні змінні\n\nexport default {\n  __name: 'HomePrihod',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const orders = ref(ordersData);\n    const products = ref(productsData);\n    const selectedOrder = ref(null);\n    const productInfo = computed(() => {\n      return products.value.reduce((acc, product) => {\n        acc[product.id] = {\n          photo: product.photo,\n          price: product.price?.map(p => `${p.value} ${p.symbol}`).join(', ') || 'No price'\n        };\n        return acc;\n      }, {});\n    });\n    const getProductCount = orderId => {\n      const order = orders.value.find(order => order.id === orderId);\n      return order ? order.productIds.length : 0;\n    };\n    const formatDate = (date, formatStr, locale) => format(parseISO(date), formatStr, {\n      locale\n    });\n    const formatShortDate = date => formatDate(date, 'MM/dd');\n    const formatLongDate = date => formatDate(date, 'dd/MMM/yyyy', ru);\n    const selectOrder = order => selectedOrder.value = order;\n    const __returned__ = {\n      orders,\n      products,\n      selectedOrder,\n      productInfo,\n      getProductCount,\n      formatDate,\n      formatShortDate,\n      formatLongDate,\n      selectOrder,\n      ref,\n      computed,\n      get format() {\n        return format;\n      },\n      get parseISO() {\n        return parseISO;\n      },\n      get ru() {\n        return ru;\n      },\n      get ordersData() {\n        return ordersData;\n      },\n      get productsData() {\n        return productsData;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","format","parseISO","ru","ordersData","productsData","orders","products","selectedOrder","productInfo","value","reduce","acc","product","id","photo","price","map","p","symbol","join","getProductCount","orderId","order","find","productIds","length","formatDate","date","formatStr","locale","formatShortDate","formatLongDate","selectOrder"],"sources":["/home/vadim/my-test/src/views/HomePrihod.vue"],"sourcesContent":["<template>\n<section class=\"orders\">\n  <div class=\"orders__container\">\n    <ul>\n      <li \n        class=\"orders__item\"\n        :class=\"selectedOrder && selectedOrder.productIds.length > 0 ? 'orders__item--compact' : 'orders__item--full'\"\n        v-for=\"order in orders\"\n        :key=\"order.id\"\n        @click=\"selectOrder(order)\"\n      >\n        <h2 class=\"orders__title\" v-if=\"!selectedOrder || selectedOrder.productIds.length === 0\">\n          Длинное название прихода\n        </h2>\n        <div class=\"orders__info_first\">\n          <p>{{ getProductCount(order.id) }}</p>\n          <span>Продукты</span>\n        </div>\n        <div class=\"orders__info\">\n          <time>{{ formatShortDate(order.date) }}</time>\n          <time>{{ formatLongDate(order.date) }}</time>\n        </div>\n        <p class=\"orders__price\" v-if=\"!selectedOrder || selectedOrder.productIds.length === 0\">\n          {{ order.productIds.length > 0 ? productInfo[order.productIds[0]].price || 'No price' : '' }}\n        </p>\n        <img \n          v-if=\"!selectedOrder || selectedOrder.productIds.length === 0\" \n          class=\"orders__item_remove\" \n          src=\"@/assets/remove.png\" \n          alt=\"remove\"\n        >\n      </li>\n    </ul>\n  </div>\n  <transition name=\"fade\">\n    <aside v-if=\"selectedOrder && selectedOrder.productIds.length > 0\" class=\"orders__product-details\">\n        <h2>Длиное название прихода</h2>\n        <section>\n            <button type=\"button\">Добавить продукт