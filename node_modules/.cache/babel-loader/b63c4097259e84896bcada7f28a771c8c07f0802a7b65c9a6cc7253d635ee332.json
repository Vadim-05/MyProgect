{"ast":null,"code":"import { format } from 'date-fns';\nimport { uk } from 'date-fns/locale';\nexport default {\n  __name: 'todayDate',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const now = new Date();\n    const __returned__ = {\n      now,\n      get format() {\n        return format;\n      },\n      get uk() {\n        return uk;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["format","uk","now","Date"],"sources":["/home/vadim/Desktop/MyProgect/src/components/todayDate.vue"],"sourcesContent":["<template>\n  <section class=\"date-time\">\n    <p class=\"date-time__day\">\n      {{ now.toLocaleDateString('uk-UA', { weekday: 'long' }) }}\n    </p>\n    <p class=\"date-time__details\">\n      <span class=\"date-time__date\">\n        {{ format(now, 'dd MMM, yyyy', { locale: uk }) }}\n      </span>\n      <img src='@/assets/clock.png' class=\"date-time__icon\" alt='clock'/>\n      <span class=\"date-time__time\">\n        {{ format(now, 'HH:mm') }}\n      </span>\n    </p>\n  </section>\n</template>\n\n<script setup>\nimport { format } from 'date-fns';\nimport { uk } from 'date-fns/locale';\n\nconst now = new Date();\n</script>\n<style lang=\"scss\">\n.date-time{\n  &__details{\n    display: flex;\n    align-items: center;\n  }\n  &__icon{\n    width: 20px;\n    padding: 0 10px 0 20px;\n  }\n}\n</style>"],"mappings":"AAkBA,SAASA,MAAM,QAAQ,UAAU;AACjC,SAASC,EAAE,QAAQ,iBAAiB;;;;;;;IAEpC,MAAMC,GAAG,GAAG,IAAIC,IAAI,CAAC,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}