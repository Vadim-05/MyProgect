{"ast":null,"code":"import { ref, computed } from 'vue';\nimport { format, parseISO } from 'date-fns';\nimport { ru } from 'date-fns/locale';\nimport ordersData from '../data/orders.json';\nimport productsData from '../data/products.json';\n\n// Створюємо реактивні змінні\n\nexport default {\n  __name: 'HomePrihod',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const orders = ref(ordersData);\n    const products = ref(productsData);\n    const selectedOrder = ref(null);\n\n    // Функція для отримання даних про продукт\n    const productInfo = computed(() => {\n      return products.value.reduce((acc, product) => {\n        acc[product.id] = {\n          title: product.title || 'No product',\n          price: product.price?.map(p => `${p.value} ${p.symbol}`).join(', ') || 'No price'\n        };\n        return acc;\n      }, {});\n    });\n    const getProductCount = orderId => {\n      const order = orders.value.find(order => order.id === orderId);\n      return order ? order.productIds.length : 0;\n    };\n    const formatDate = (date, formatStr, locale) => format(parseISO(date), formatStr, {\n      locale\n    });\n    const formatShortDate = date => formatDate(date, 'MM/dd');\n    const formatLongDate = date => formatDate(date, 'dd/MMM/yyyy', ru);\n    const selectOrder = order => selectedOrder.value = order;\n    const __returned__ = {\n      orders,\n      products,\n      selectedOrder,\n      productInfo,\n      getProductCount,\n      formatDate,\n      formatShortDate,\n      formatLongDate,\n      selectOrder,\n      ref,\n      computed,\n      get format() {\n        return format;\n      },\n      get parseISO() {\n        return parseISO;\n      },\n      get ru() {\n        return ru;\n      },\n      get ordersData() {\n        return ordersData;\n      },\n      get productsData() {\n        return productsData;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","format","parseISO","ru","ordersData","productsData","orders","products","selectedOrder","productInfo","value","reduce","acc","product","id","title","price","map","p","symbol","join","getProductCount","orderId","order","find","productIds","length","formatDate","date","formatStr","locale","formatShortDate","formatLongDate","selectOrder"],"sources":["/home/vadim/my-test/src/views/HomePrihod.vue"],"sourcesContent":["<template>\n  <div class=\"vdsafvds\">\n    <div class=\"orders-container\">\n      <transition-group name=\"slide\" tag=\"ul\">\n        <li class=\"order-item\" v-for=\"order in orders\" :key=\"order.id\" @click=\"selectOrder(order)\">\n          <span class=\"order-title\">Длиное название прихода</span>\n          <div class=\"order-info\">\n            <div>Кількість продуктів: {{ getProductCount(order.id) }}</div>\n            <div>Продукти</div>\n          </div>\n          <div class=\"order-info\">\n            <div>{{ formatShortDate(order.date) }}</div>\n            <div>{{ formatLongDate(order.date) }}</div>\n          </div>\n        </li>\n      </transition-group>\n    </div>\n\n    <!-- Блок для деталей продукту -->\n    <transition name=\"fade\">\n      <div class=\"product-details\" v-if=\"selectedOrder\">\n        <ul>\n          <li v-for=\"productId in selectedOrder.productIds\" :key=\"productId\" class=\"product-item\">\n            <div>{{ productInfo[productId].title || 'No product' }}</div>\n            <div class=\"product-price\">{{ productInfo[productId].price || 'No price' }}</div>\n          </li>\n        </ul>\n      </div>\n    </transition>\n  </div>\n</template>\n\n\n<script setup>\nimport { ref, computed } from 'vue';\nimport { format, parseISO } from 'date-fns';\nimport { ru } from 'date-fns/locale';\nimport ordersData from '../data/orders.json';\nimport productsData from '../data/products.json';\n\n// Створюємо реактивні змінні\nconst orders = ref(ordersData);\nconst products = ref(productsData);\nconst selectedOrder = ref(null);\n\n// Функція для отримання даних про продукт\nconst productInfo = computed(() => {\n  return products.value.reduce((acc, product) => {\n    acc[product.id] = {\n      title: product.title || 'No product',\n      price: product.price?.map(p => `${p.value} ${p.symbol}`).join(', ') || 'No price'\n    };\n    return acc;\n  }, {});\n});\n\nconst getProductCount = (orderId) => {\n  const order = orders.value.find(order => order.id === orderId);\n  return order ? order.productIds.length : 0;\n};\n\nconst formatDate = (date, formatStr, locale) => format(parseISO(date), formatStr, { locale });\n\nconst formatShortDate = (date) => formatDate(date, 'MM/dd');\nconst formatLongDate = (date) => formatDate(date, 'dd/MMM/yyyy', ru);\n\nconst selectOrder = (order) => selectedOrder.value = order;\n</script>\n\n\n<style scoped>\n.orders-container {\n  display: flex;\n  overflow: hidden; /* Ensure no scrollbars during transitions */\n}\n\n.vdsafvds {\n  display: flex;\n  flex-direction: column; /* Ensure children stack vertically */\n}\n\n.order-item {\n  display: flex;\n  padding: 10px;\n  border: 1px solid #ddd;\n  margin-bottom: 10px;\n  cursor: pointer;\n  position: relative; /* Required for smooth transitions */\n}\n\n/* Animation for sliding in and out */\n.slide-enter-active, .slide-leave-active {\n  transition: transform 0.5s ease;\n}\n\n.slide-enter {\n  transform: translateX(100%); /* Slide in from the right */\n}\n\n.slide-leave-to {\n  transform: translateX(-100%); /* Slide out to the left */\n}\n\n.slide-leave {\n  transform: translateX(0); /* Maintain original position until transition starts */\n}\n\n/* Adjust as needed for the opposite direction */\n.product-details {\n  padding: 20px;\n  background-color: #f9f9f9; /* Легкий фон для деталей продукту */\n  border-top: 1px solid #ddd;\n}\n\n/* Fade transition for the product details */\n.fade-enter-active, .fade-leave-active {\n  transition: opacity 0.5s ease;\n}\n\n.fade-enter, .fade-leave-to {\n  opacity: 0;\n}\n</style>\n\n"],"mappings":"AAkCA,SAASA,GAAG,EAAEC,QAAQ,QAAQ,KAAK;AACnC,SAASC,MAAM,EAAEC,QAAQ,QAAQ,UAAU;AAC3C,SAASC,EAAE,QAAQ,iBAAiB;AACpC,OAAOC,UAAU,MAAM,qBAAqB;AAC5C,OAAOC,YAAY,MAAM,uBAAuB;;AAEhD;;;;;;;;IACA,MAAMC,MAAM,GAAGP,GAAG,CAACK,UAAU,CAAC;IAC9B,MAAMG,QAAQ,GAAGR,GAAG,CAACM,YAAY,CAAC;IAClC,MAAMG,aAAa,GAAGT,GAAG,CAAC,IAAI,CAAC;;IAE/B;IACA,MAAMU,WAAW,GAAGT,QAAQ,CAAC,MAAM;MACjC,OAAOO,QAAQ,CAACG,KAAK,CAACC,MAAM,CAAC,CAACC,GAAG,EAAEC,OAAO,KAAK;QAC7CD,GAAG,CAACC,OAAO,CAACC,EAAE,CAAC,GAAG;UAChBC,KAAK,EAAEF,OAAO,CAACE,KAAK,IAAI,YAAY;UACpCC,KAAK,EAAEH,OAAO,CAACG,KAAK,EAAEC,GAAG,CAACC,CAAC,IAAI,GAAGA,CAAC,CAACR,KAAK,IAAIQ,CAAC,CAACC,MAAM,EAAE,CAAC,CAACC,IAAI,CAAC,IAAI,CAAC,IAAI;QACzE,CAAC;QACD,OAAOR,GAAG;MACZ,CAAC,EAAE,CAAC,CAAC,CAAC;IACR,CAAC,CAAC;IAEF,MAAMS,eAAe,GAAIC,OAAO,IAAK;MACnC,MAAMC,KAAK,GAAGjB,MAAM,CAACI,KAAK,CAACc,IAAI,CAACD,KAAK,IAAIA,KAAK,CAACT,EAAE,KAAKQ,OAAO,CAAC;MAC9D,OAAOC,KAAK,GAAGA,KAAK,CAACE,UAAU,CAACC,MAAM,GAAG,CAAC;IAC5C,CAAC;IAED,MAAMC,UAAU,GAAGA,CAACC,IAAI,EAAEC,SAAS,EAAEC,MAAM,KAAK7B,MAAM,CAACC,QAAQ,CAAC0B,IAAI,CAAC,EAAEC,SAAS,EAAE;MAAEC;IAAO,CAAC,CAAC;IAE7F,MAAMC,eAAe,GAAIH,IAAI,IAAKD,UAAU,CAACC,IAAI,EAAE,OAAO,CAAC;IAC3D,MAAMI,cAAc,GAAIJ,IAAI,IAAKD,UAAU,CAACC,IAAI,EAAE,aAAa,EAAEzB,EAAE,CAAC;IAEpE,MAAM8B,WAAW,GAAIV,KAAK,IAAKf,aAAa,CAACE,KAAK,GAAGa,KAAK","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}